{
  "name": "objectro",
  "version": "0.1.1",
  "description": "Transform and validate objects",
  "author": "Matt Scheurich <matt@lvl99.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "lint": "eslint . --ignore-path .lintignore",
    "lint:fix": "eslint . --ignore-path .lintignore",
    "test": "jest",
    "test:watch": "jest --watch",
    "deploy": "yarn test && yarn build"
  },
  "keywords": [
    "javascript",
    "object",
    "transform",
    "destructure",
    "restructure",
    "format",
    "filter",
    "validate"
  ],
  "main": "dist/objectro.js",
  "module": "index.js",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "lint-staged": "^8.0.4",
    "lodash": "^4.17.11",
    "prettier": "^1.14.3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  },
  "husky": {
    "hooks": {
      "post-commit": "git update-index --again",
      "pre-commit": "lint-staged",
      "post-merge": "yarn",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "git add"
    ]
  }
}
